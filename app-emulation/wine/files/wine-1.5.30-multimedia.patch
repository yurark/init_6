+static const struct target_info * get_target_info(const char *target)
+{
+    LONG min = 0;
+    LONG max = sizeof(targets_info) / sizeof(targets_info[0]) - 1;
+    LONG cur;
+    int res;
+
+    while (min <= max)
+    {
+        cur = (min + max) / 2;
+        res = strcmp(target, targets_info[cur].name);
+        if (res < 0)
+            max = cur - 1;
+        else if (res > 0)
+            min = cur + 1;
+        else
+            return &targets_info[cur];
+    }
+
+    return NULL;
+}
